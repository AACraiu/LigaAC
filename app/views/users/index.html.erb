<h1> Users </h1>

<ul>
    <% @users.each do |user| %>
        <li style="border-bottom: 1px solid black">
            name: <%= user.name %><br>
            email: <%= user.email %><br>
            age: <%= user.age %><br>
            gender: <%= user.gender %>
        </li>
    <% end %>
</ul>

